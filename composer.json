{
	"name" : "goetas/xsd2php",
	"description" : "Convert XSD  (XML Schema) definitions into PHP classes",
	"type" : "library",
	"authors" : [{
			"name" : "Asmir Mustafic"
		}
	],
	"keywords" : [
		"converter",
		"xml",
		"xsd",
		"php"
	],
	"license" : [
		"LGPL"
	],
	"require" : {
		"symfony/console" : "2.*",
		"ext-xsl" : "*",
		"php" : ">=5.3.2",
		"symfony/class-loader" : "2.*"
	},
	"autoload" : {
		"psr-4" : {
			"Goetas\\Xsd\\XsdToPhp\\" : "lib/"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "2.0-dev"
		}
	},
    "bin": [
	    "bin/xsd2php"
    ]
}